<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="yerim.dao.face.MypageDao">

	<select id="selectUserInfo" parameterType="int" resultType="Users">
		SELECT user_name,user_nick,user_email,user_id,user_phone,user_gender,user_birth,user_job,user_address
		FROM users
		WHERE user_no = #{userNo}
	</select>

</mapper>