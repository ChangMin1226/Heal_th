<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace="yerim.dao.face.LoginDao">
    	<insert id="insertJoinInfo" parameterType="Users">
    	
    		INSERT INTO users (user_no,user_id,user_pw,user_nick,user_phone,user_address,user_email,user_name,user_joindate,ranking_no,point)
    		VALUES(users_seq.nextval,#{userId},#{userPw},#{userNick},#{userPhone},#{userAddress},#{userEmail},#{userName},sysdate,1,1000)
    	
    	</insert>
    	
    	
    
    
    </mapper>